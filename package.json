{
  "name": "expandhealth-v2",
  "version": "2.0.0",
  "description": "ExpandHealth Web Portal - Full practice management platform with AI",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "lint": "eslint server.js api/ services/ middleware/ database/",
    "lint:security": "eslint --rule 'security/detect-eval-with-expression: error' server.js api/ services/ middleware/ database/",
    "test": "npm run test:all",
    "test:frontend": "jest --config jest.frontend.config.js --coverage",
    "test:backend": "jest --testPathIgnorePatterns=public/js --coverage",
    "test:all": "npm run test:backend && npm run test:frontend",
    "migrate:status": "node database/migrate-status.js",
    "migrate:create": "node scripts/create-migration.js",
    "migrate:lint": "node scripts/lint-migrations.js",
    "smoke-test": "node scripts/smoke-test.js"
  },
  "keywords": [
    "healthcare",
    "ai",
    "practice-management",
    "treatment-planning"
  ],
  "author": "ExpandHealth",
  "license": "MIT",
  "engines": {
    "node": ">=18.14.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.73.0",
    "@axiomhq/pino": "^1.4.0",
    "@bull-board/api": "^6.6.1",
    "@bull-board/express": "^6.6.1",
    "@google/genai": "^1.40.0",
    "@sentry/node": "^10.38.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.67.3",
    "cors": "^2.8.6",
    "docx": "^9.5.1",
    "dotenv": "^17.2.4",
    "ejs": "^4.0.1",
    "express": "^5.2.1",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "formidable": "^3.5.4",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "openai": "^6.18.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "2.4.5",
    "pdfjs-dist": "^5.4.624",
    "pg": "^8.18.0",
    "pino": "^10.3.0",
    "pino-http": "^11.0.0",
    "rate-limit-redis": "^4.3.1",
    "resend": "^6.9.1",
    "twilio": "^5.12.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/dom": "^10.4.1",
    "@types/pdfkit": "^0.17.5",
    "axios": "^1.13.5",
    "eslint": "^9.39.2",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "pdfkit": "^0.17.2",
    "pino-pretty": "^13.1.3",
    "supertest": "^7.2.2"
  }
}
